<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Item">
  <TemplateData>
    <ProjectType>CSharp</ProjectType>

    <DefaultName>IntegrationTest.cs</DefaultName>
    <Name>CRM Plug-in Integration Test</Name>
    <Description>Dynamics CRM Plug-in Integration Test</Description>
    <Icon>icon.png</Icon>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <References />

    <ProjectItem TargetFileName="$fileinputname$.cs" ReplaceParameters="true">IntegrationTest1.cs</ProjectItem>

  </TemplateContent>
  <WizardExtension>
    <Assembly>TemplateWizards, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null</Assembly>
    <FullClassName>TemplateWizards.VersionWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <!--Valid values are Plug-in or Workflow or Item-->
    <CRMProjectType>Item</CRMProjectType>
    <NeedsClient>False</NeedsClient>
    <IsUnitTest>False</IsUnitTest>
    <IsUnitTestItem>True</IsUnitTestItem>
  </WizardData>
</VSTemplate>